{
  "connection": "default",
  "collectionName": "annonces",
  "info": {
    "name": "Annonce"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Titre": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Adresse": {
      "type": "string"
    },
    "DateLimite": {
      "type": "date"
    },
    "Photo": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "DatePickup": {
      "type": "datetime"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "Picture": {
      "type": "string"
    },
    "Categorie": {
      "type": "string"
    },
    "Statut_Annonce": {
      "type": "string",
      "default": "Disponible"
    },
    "Statut_Reservation": {
      "type": "string"
    },
    "Reserving_User": {
      "type": "string",
      "default": "null"
    }
  }
}
